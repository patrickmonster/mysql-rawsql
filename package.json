{
    "name": "mysql-rowquery",
    "version": "1.0.6",
    "description": "mysql row query",
    "main": "dist/cjs/index.js",
    "module": "dist/mjs/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup.sh"
    },
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "author": "Soung jin Ryu <neocat@kakao.com>",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^22.13.1",
        "typescript": "^5.7.3"
    },
    "prettier": {
        "trailingComma": "es5",
        "arrowParens": "avoid",
        "endOfLine": "lf",
        "singleQuote": true,
        "semi": true,
        "bracketSpacing": true,
        "printWidth": 120,
        "tabWidth": 4
    },
    "dependencies": {
        "mysql2": "^3.12.0"
    }
}
